{
  "providers": {
    "claude-code": {
      "enabled": true,
      "priority": 1,
      "timeout": 120000,
      "command": "claude"
    },
    "gemini-cli": {
      "enabled": true,
      "priority": 2,
      "timeout": 180000,
      "command": "gemini"
    }
  },
  "memory": {
    "maxEntries": 10000,
    "persistPath": ".automatosx/memory",
    "autoCleanup": true,
    "cleanupDays": 30
  },
  "workspace": {
    "basePath": ".automatosx/workspaces",
    "autoCleanup": true,
    "cleanupDays": 7,
    "maxFiles": 100
  },
  "logging": {
    "level": "debug",
    "path": ".automatosx/logs",
    "console": true
  },
  "$schema": "https://automatosx.dev/schema/config.json",
  "version": "4.0.0"
}